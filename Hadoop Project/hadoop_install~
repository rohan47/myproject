#!/usr/bin/python

import os,time


def main():
	install_exec()
	

def install_exec():			
	tmp=open('hosts.txt','r')
	v=tmp.read()
	x=v.split('\n')
	for line in x:
		
		os.system('sshpass -p \'redhat\' ssh -o StrictHostKeyChecking=no root@'+line+' ./tmp/installer')
		print line+'done!!'
		
		

	tmp.close()
	

	
	
main()
